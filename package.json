{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "",
  "browserslist": [
    "last 2 versions",
    "> 5%"
  ],
  "scripts": {
    "build": "webpack --config configs/webpack.dev.config.js --mode=development",
    "watch": "webpack --config configs/webpack.dev.config.js --mode=development --watch",
    "bundle": "npm install && npm run watch",
    "dev": "webpack serve --config configs/webpack.dev.config.js --mode=development",
    "production": "webpack --config configs/webpack.prod.config.js --mode=production",
    "stats": "webpack --config configs/webpack.prod.config.js --mode=production --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json"
  },
